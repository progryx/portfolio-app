{
  "compilerOptions": {
    "noEmit": true,
    "jsx": "react",
    "skipLibCheck": true,
    "strict": true,
    "sourceMap": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true,
    "allowJs": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "removeComments": true,
  },
  "exclude": ["**/node_modules/*"],
}
