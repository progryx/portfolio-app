  "scripts": {
    "start": "webpack serve --mode=development --env=development --port=3000",
    "build": "webpack --mode=development",
    "build:prod": "webpack --mode=production",
    "lint": "npm-run-all --parallel lint:*",
    "lint:ts": "eslint \"src/**/*.ts{,x}\"",
    "lint:css": "stylelint \"src/**/*.scss\"",
    "lint-fix": "npm-run-all format lint-fix:*",
    "lint-fix:ts": "eslint \"src/**/*.ts{,x}\" --fix",
    "lint-fix:css": "stylelint \"src/**/*.scss\" --fix",
    "format": "prettier --write \"src/*.{json,{j,t}s{,x},scss,}\"",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && concurrently yarn:lint"
    }
  },
  "devDependencies": {
    "@portfolio-app/linters": "1.0.0",
