    "@types/react-dom": "17.0.3",
    "@types/react-google-recaptcha": "^2.1.0",
    "@types/react-redux": "^7.1.9",
    "classnames": "^2.2.6",
    "emailjs-com": "^2.6.3",
    "print-js": "^1.5.0",
    "react": "17.0.2",
